use (
  submodule 
  string
)

give (
  check
)

def check = fun x =>
  x |> decorate("module_1") |> sumbmodule.check

def decorate = fun x =>
  string.concat("module_1 ", x)





let string = module string
and let submodule = module submodule
in
  module giving check where
    def check = fun x =>
        x |> decorate("module_1") |> sumbmodule.check

    def decorate = fun x =>
        string.concat("module_1 ", x)

