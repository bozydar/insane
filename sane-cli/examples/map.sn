let map = fun fn =>
                let map_ = fun list =>
                if app (app list to count), 0 to eq then
                    []
                else
                    let h = app (app list to head) to fn in
                    let t = app list to tail in
                    app [h], (app t to map_) to concat
                in map_
            in
                app [1; 2; 3; "chuj"; 5] to app (fun a => app a to inc) to map
