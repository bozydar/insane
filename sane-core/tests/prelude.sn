give (
    map
)

def reduce = fun fn =>
  let reduce_0 = fun acc list =>
    if list |> count |> eq <| 0 then
      acc
    else
      let h = list |> head
      in
        let t = list |> tail
        in
          let new_acc = fn(h acc)
          in
            reduce_0(new_acc t)
  in
    reduce_0
